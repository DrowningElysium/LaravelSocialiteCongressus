{
    "name": "drowningelysium/laravel-socialite-congressus",
    "description": "An oAuth2 implementation to allow authentication through Congressus.",
    "type": "library",
    "keywords": ["framework", "laravel"],
    "license": "proprietary",
    "require": {
        "php": "^7.2 | ^8.0",
        "ext-json": "*",
        "laravel/socialite": "^v5.5"
    },
    "autoload": {
        "psr-4": {
            "DrowningElysium\\LaravelSocialiteCongressus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DrowningElysium\\LaravelSocialiteCongressus\\CongressusServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
